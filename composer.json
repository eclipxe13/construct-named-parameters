{
    "name": "eclipxe/construct-named-parameters",
    "description": "Helper functions to create objects using an array as named parameters",
    "homepage": "https://github.com/eclipxe13/construct-named-parameters",
    "license": "MIT",
    "keywords": ["named parameters"],
    "authors": [
        {
            "name": "Carlos C Soto",
            "email": "eclipxe13@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "jakub-onderka/php-parallel-lint": "^0.9.2",
        "squizlabs/php_codesniffer": "^3.0",
        "friendsofphp/php-cs-fixer": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "ConstructNamedParameters\\": "src/ConstructNamedParameters/"
        },
        "files": [
            "src/helper_functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\ConstructNamedParameters\\": "tests/ConstructNamedParameters/"
        }
    }
}
